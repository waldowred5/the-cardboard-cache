<p id="notice"><%= notice %></p>

<h1>Owned Games</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @owned_games.each do |owned_game| %>
      <tr>
        <td><%= link_to 'Show', owned_game %></td>
        <td><%= link_to 'Edit', edit_owned_game_path(owned_game) %></td>
        <td><%= link_to 'Destroy', owned_game, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Owned Game', new_owned_game_path %>
